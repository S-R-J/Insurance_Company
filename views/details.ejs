<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@3.3.7/dist/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
</head>

<style>
  .container
    {
        display: flex;
        justify-content: center;
        align-items: center;
        text-align: center;
        min-height: 100vh;
        /* border: 2px black solid; */
    
    }

    .ttt
    {
        display:grid;
        align-self: center;
        /* grid-template-rows: repeat(6, 1fr); */
        border:2px solid black;
        border-radius: 17px;
        /* min-height: 50vh; */
        /* min-width: 50vw; */
    }


</style>


<body>

  <div class="container">
    <div class="ttt">
      <table class="table">
        <thead>
          <tr>
            <th scope="col">Name</th>
            <th scope="col">Number</th>
            <th scope="col">Password</th>
            <th scope="col">Age</th>
            <th scope="col">Gender</th>
            <th scope="col">Education</th>
            <th scope="col">Income</th>
            <th scope="col">Occupation</th>
            <th scope="col">Smoke</th>
            <th scope="col">HLV</th>
            <th scope="col">Cover</th>
            <th scope="col">Life Ins. Year</th>
            <th scope="col">Term</th>
            <th scope="col">Life Ins. Premium</th>
            <th scope="col">Bike Ins. Premium</th>
            <th scope="col">Bike Ins. Year</th>
            <th scope="col">Bike Number</th>
          </tr>
        </thead>
        <tbody>
        <% detailList.forEach(detail => {%>
            <tr>
                <!-- <th scope="row"></th> -->
                <td><%= detail.name %></td>
                <td><%= detail.number %></td>
                <td><%= detail.password %></td>
                <td><%= detail.age %></td>
                <td><%= detail.gender %></td>
                <td><%= detail.education %></td>
                <td><%= detail.income %></td>
                <td><%= detail.occupation %></td>
                <td><%= detail.smoke %></td>
                <td><%= detail.hlv %></td>
                <td><%= detail.cover %></td>
                <td><%= detail.lifeyear %></td>
                <td><%= detail.term %></td>
                <td><%= detail.lifepremium %></td>
                <td><%= detail.bikepremium %></td>
                <td><%= detail.bikeyear %></td>
                <td><%= detail.bikenumber %></td>
              </tr>
        <%})%>
          
        </tbody>
      </table>
    </div>
    
  </div>
    
    
</body>
</html>